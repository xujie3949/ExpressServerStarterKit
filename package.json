{
  "name": "DBServer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "shx rm -rf ./dist",
    "compile": "npm run clean && babel src -d dist -D",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test -- --watch",
    "lint": "eslint ./src ./tests",
    "lint:fix": "npm run lint -- --fix"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node ./src/bin/server.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "*"
      }
    },
    "start": {
      "command": "npm run compile && node ./dist/bin/server.js",
      "env": {
        "DEBUG": "*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "*"
      }
    }
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "morgan": "~1.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.11",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.3.0",
    "cheerio": "^0.20.0",
    "codecov": "^1.0.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "karma": "^1.0.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "mocha": "^3.0.1",
    "nodemon": "^1.10.2",
    "shx": "^0.1.4",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "transform-loader": "^0.2.3"
  }
}
